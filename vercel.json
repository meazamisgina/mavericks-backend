{
  "version": 2,
  "builds": [
    {
      "src": "mitumbaesales/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mitumbaesales/wsgi.py"
    }
  ]
}